<% if flash[:duck_errors]%>
<% flash[:duck_errors].each do |error|%>
<%= error %>
    <% end %>
<% end %>


<h1> New Duck Form </h3>
<%= form_for @duck do |f| %>
<%= f.label :name %><br>
<%= f.text_field :name %><br>
<%= f.label :description %><br>
<%= f.text_area :description%><br>
<%= f.collection_select :student_id, Student.all, :id, :name %><br>
<%= f.submit%>
<% end %>